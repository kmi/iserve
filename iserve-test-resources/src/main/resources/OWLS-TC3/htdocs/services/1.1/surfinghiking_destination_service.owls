<rdfs:label></rdfs:label>
</process:Input>

<process:Output  rdf:ID="_DESTINATION">
<process:parameterType rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://127.0.0.1/ontology/travel.owl#Destination</process:parameterType>
<rdfs:label></rdfs:label>
</process:Output>

